{
    "name": "wdevstudio/laravel-chat-messenger",
    "description": "Laravel chat package",
    "license": "MIT",
    "keywords": [
        "message",
        "inbox",
        "conversations",
        "chat",
        "php",
        "laravel",
        "realtime",
        "real-time",
        "talk",
        "fb chat",
        "like facebook chat",
        "like facebook messenger",
	"support chat"
    ],
    "authors": [
        {
            "name": "Pavel Palashturov",
            "email": "support@wdevstudio.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wdevstudio/laravel-chat-messenger"
        }
    ],
    "require": {
        "php": ">=7.0",
    	"illuminate/support": "~5",
	"pusher/pusher-php-server": ">=2.6"
    },
    "autoload": {
        "psr-4": {
            "wDevStudio\\LaravelMessenger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "wDevStudio\\LaravelMessenger\\LaravelMessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "wDevStudio\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    }
}
